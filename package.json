{
  "$schema": "https://json.schemastore.org/package",
  "name": "@xeho91/colors",
  "version": "1.1.1",
  "description": "xeho91's brand colors with custom CLI for automated tasks.",
  "keywords": [
    "colors",
    "brand assets",
    "automation",
    "CSS",
    "JSON",
    "CLI",
    "Deno"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Mateusz Kadlubowski",
    "email": "xeho91@pm.me",
    "url": "https://xeho91.com/"
  },
  "homepage": "https://github.com/xeho91/colors#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeho91/colors.git"
  },
  "bugs": {
    "url": "https://github.com/xeho91/colors/issues"
  },
  "files": [
    "/bin/**/*",
    "/dist/**/*",
    "/source/**/*"
  ],
  "bin": {
    "xeho91-colors": "./bin/cli"
  },
  "main": "/dist/colors.css",
  "style": "/dist/colors.css",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build:css": "./bin/cli build css --variant hsl --output ./dist",
    "build:json": "./bin/cli build json --output ./dist",
    "release": "standard-version --verbose"
  },
  "devDependencies": {
    "standard-version": "^9.3.0"
  }
}
